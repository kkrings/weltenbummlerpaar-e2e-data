{
  "name": "@kkrings/weltenbummlerpaar-e2e-data",
  "version": "1.0.2",
  "description": "Node package for filling data for E2E testing into MongoDB",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "scripts": {
    "prepare": "husky install",
    "prebuild": "rimraf dist",
    "build": "tsc --project tsconfig.build.json",
    "test": "jest",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kkrings/weltenbummlerpaar-e2e-data.git"
  },
  "author": "Kai Krings",
  "license": "GPL-3.0",
  "bugs": "https://github.com/kkrings/weltenbummlerpaar-e2e-data/issues",
  "homepage": "https://github.com/kkrings/weltenbummlerpaar-e2e-data#readme",
  "dependencies": {
    "mongodb": "^4.6.0"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "^16.11.36",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.2",
    "typescript": "^4.6.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "jest": true,
      "es2021": true
    },
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "tsconfig.json",
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {}
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "publishConfig": {
    "@kkrings:registry": "https://npm.pkg.github.com"
  }
}
